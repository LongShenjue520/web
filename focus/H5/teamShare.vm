	            dataType: 'json',
	            cache: false,
	            success:function(jsondata){ 
	              if(jsondata.retCode =='0'){
	                console.log(JSON.stringify(jsondata.result.totalAmount))
	                that.redBao = jsondata.result.list
	                // console.log(that.redBao);
	                var totalRed = jsondata.result.totalAmount+"";
	                var cnt = 0;
	                var str="<b>已领取</b>";
	                if(totalRed.length < 6) {
	                    for(var i=0;i<6-totalRed.length;i++) {
	                        str += "<span>"+0+"</span>"; 
	                    }
	                }
	                
	                for(var index=0;index<totalRed.length;index++) {
	                    str += "<span>"+totalRed.charAt(index)+"</span>"; 
	                }
	                str+="<b>元红包</b>";
	                $('.My_redPackets_top div').html(str);
	                setTimeout(function(){
	                  $.fn.scrollTop = function(options){
	                      var defaults = {
	                          speed:30
	                      }
	                      var opts = $.extend(defaults,options);
	                      this.each(function(){
	                          var $timer;
	                          var scroll_top=0;
	                          var obj = $(this);
	                          var $height = obj.find("ul").height();
	                          obj.find("ul").clone(true).appendTo(obj);
	                          obj.hover(function(){
	                              clearInterval($timer);
	                          },function(){
	                              $timer = setInterval(function(){
	                                  scroll_top++;
	                                  if(scroll_top > $height){
	                                      scroll_top = 0;
	                                  }
	                                  obj.find("ul").first().css("margin-top",-scroll_top);
	                              },opts.speed);
	                          }).trigger("mouseleave");
	                      })
	                  };
	                  $(".text").scrollTop({
	                      speed:100 //数值越大 速度越慢
	                  });
	                },1000) 

	              }
	            }
	          })
				setTimeout(function(){
					$(".gif").hide();
					$('.innerBox').show();
					$(".join_team").show();					
				},1000)
				

			},
