<script type="text/javascript">
	var app = new Vue({
		el:'#app',
		data:{
			my_list:{},
			my_list_month:{},
			teamList:[],
			teamTotal:[],
			teamMonth:[],	
			redBao:{}	
		},
		beforeCreate:function(){
			// $(".gif").show();
			// $('.innerBox').hide();
		},
		created:function(){
			// $(".gif").show();
			// $('.innerBox').hide();
		},
		computed:{},
		mounted:function(){
			var that = this;
				if(window.localStorage.getItem("ssoId")){
					var ssoId = window.localStorage.getItem("ssoId");
					var token = window.localStorage.getItem("token");
					that.ssoId = ssoId;
					console.log(ssoId,token)
				}
				// 获取用户id 
				teamId = window.location.href.match(/teamId=\w+/)[0];
                teamId = teamId.replace('teamId=','');
				//  查询战队的详细信息
				$.ajax({
					url: 'http://itougu.jrj.com.cn/match/v7/team/queryTeamDetail.jspa',
					data:{
						userId:ssoId,
						teamId:Number(teamId)
					},
					type: 'get',
					dataType: 'json',
					success:function(jsondata){	
						if(jsondata.retCode=='0'){
							that.teamList = jsondata.teamDetail;
							console.log(jsondata.teamDetail)
							$('title').html(jsondata.teamDetail.teamName);
							document.title = jsondata.teamDetail.teamName ;
							var i = 0; 
							var ms = 4000; //变量MS: 从0%到100%需要的毫秒数
							var num = jsondata.teamDetail.tradeCapital/5
							var time = setInterval(function(){
								$(".progress .finish").css("width",i+"%");
								i=i+(1000/ms); 
								if(i>num){
								clearInterval(time);
							}},10); 
							console.log(JSON.stringify(that.teamList))
						}
					},
					error:function(e){
						console.log(e)
					}
				})	
			// 战队个人排行榜 总收益
