        //时间比较
        function compateDate(e, t) {
            var n = e.split("-"),
            o = new Date(parseInt(n[0]), parseInt(n[1]), parseInt(n[2])),
            r = o.getTime(),
            c = t.split("-"),
            a = new Date(parseInt(c[0]), parseInt(c[1]), parseInt(c[2])),
            i = a.getTime();
            return r - i
        };
