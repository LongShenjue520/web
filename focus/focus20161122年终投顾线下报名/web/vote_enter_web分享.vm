<script>

    window.dcsMultiTrack('DCS.dcsuri', 'gotoWebVoteEnter', 'WT.ti', 'gotoWebVoteEnter');
    var btnup=$('.btnup');
    var btnside=$('.btnside');

    btnup.click(function(){
        layer()
    });
    btnside.click(function(){
        layer()
    });
    $('.closebtn,.looklook').click(function(){
       $('.mask,.layer').hide();
    });
    function layer(){
    	//判断是否登录 
	   if (typeof basicUserInfo == 'undefined' || basicUserInfo == null || basicUserInfo.userId == null) {
		   
	        JRJ.ui.isLogin(function(){
                 var userName = basicUserInfo.userName;
                 $("#userName").text(userName);
	        })
	    } else {
	    	var userName = basicUserInfo.userName;
            $("#userName").text(userName);
	        $('.mask,.layer').show();
	    }
       
   };
   $(".layerin1 a").bind("click",function(){
	   var name = $("#name").val();
	   var mobile = $("#mobile").val();
	   var address = $("#address").val();
	   var userId = basicUserInfo.userId;
	   var userName = basicUserInfo.userName;
	   
	   if(mobile==''){
		   JRJ.MiniAlerts.prompt({message:'<p>手机号不能为空！</p>',trigger:"#mobile"});
		    return;
	   }else{
		    var reg = /^0?1[3|4|5|7|8][0-9]\d{8}$/;
		    if (!reg.test(mobile)) {
				  JRJ.MiniAlerts.prompt({message:'<p>手机号输入不正确！</p>',trigger:"#mobile"});
                  return;
		    }
	   }
	   if(name==''){
		   JRJ.MiniAlerts.prompt({message:'<p>姓名不能为空！</p>',trigger:"#name"});
		   return;
	   }
	   if(address==''){
		   JRJ.MiniAlerts.prompt({message:'<p>地址不能为空！</p>',trigger:"#address"});
		    return;
	   }else{
		   if(address.length>50){
			    JRJ.MiniAlerts.prompt({message:'<p>地址不能太长！</p>',trigger:"#address"});
			     return;
		   }
	   }
	   
	   //ajax提交
       	  $.ajax({
		   url:'http://itougu.jrj.com.cn/marketing/voteEnter.jspa',
		   type:"POST",
		   cache : false,
		   dataType : "json",
		   data : {
       	           userId:userId,
       	           userName:userName,
       	           name:name,
       	           mobile:mobile,
       	           address:address
		           },
		   success:function(data) {
		      
		        if(data.retCode=='0'){
		        	$('.layerin1').addClass('hide');
		        	$('.layerin2').removeClass('hide');
		        }else if(data.retCode=='-4'){
		        	$('.layerin1').addClass('hide');
		        	$('.layerin3').removeClass('hide');
		        }

		   },
		   error : function() {    
             alert("异常！");    
           }   
		  });
	   
   });
   
    //滚动判断报名按钮的隐藏与显示 
    var btnuptop=btnup.offset().top+btnup.height();
    
   $(window).scroll(function(e){
        if($(document).scrollTop()>=btnuptop){
            btnside.stop().css({right:0});
        }else{
            btnside.stop().css({right:'-266px'});
        }
   })

   
</script>
