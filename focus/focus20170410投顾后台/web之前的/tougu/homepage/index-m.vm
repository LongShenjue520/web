    <script type="text/javascript">
		//处理图片显示
        var wimgLI=$('.fengcai li:first').height();
        var winw=$(window).width();
        var winh=$(window).height();
        var scalewh=winw/winh;
        var resetImageWidthAndHeight = function(_obj, _imgObj) {
                _obj.attr('src', _imgObj.src);
                duibi(_obj);
                 $(_obj).unbind().click(function(){
                    $('.fengcai').css('z-index',99);
                    $('.mask,#bigimg,.closebtn').show();
                    $('#bigimg').html('<ul>'+$('#smallimg').html()+'</ul>');
                    $('#bigimg img').attr('style','');
                    var oUl=document.querySelector('#bigimg ul');
                    var aLi=oUl.children;
                    var iNow=$(_obj).parent('li').index()+1;
                    var bready=true;
                    $(oUl).html('<li>'+$(aLi[aLi.length-1]).html()+'</li>'+$(oUl).html()+'<li>'+$(aLi[0]).html()+'</li>');
                    $('#bigimg li').each(function(){
                        $(this).css('width',$('body').width()).css('height', $(window).height());
                    });
                    var x=-iNow*aLi[0].offsetWidth;
                    $('#bigimg ul').css('width',(aLi.length+2)*aLi[0].offsetWidth);
                    oUl.style.WebkitTransform='translateX('+x+'px)';
                    var haha=iNow;
                    oUl.addEventListener('touchstart',function(ev){
                        if(haha==iNow){bready=true;}
                        if(bready==false)return;
                        bready=false; 
                        
                        oUl.style.WebkitTransition='none';
                        var downX=ev.targetTouches[0].pageX;
                        var disX=downX-x;
                        function fnmove(ev){
                            x=ev.targetTouches[0].pageX-disX;
                            oUl.style.WebkitTransform='translateX('+x+'px)';
                        }
                        function fnend(ev){
                            document.removeEventListener('touchmove',fnmove,false);
                            document.removeEventListener('touchend',fnend,false);
                            var upX=ev.changedTouches[0].pageX;
                            oUl.style.WebkitTransition='.2s all ease';
                            if(Math.abs(upX-downX)>50){

                                if(downX>upX){
                                    iNow++;
                                    if(iNow==aLi.length){iNow=aLi.length-1;}
                                }else{
                                    iNow--;
                                    if(iNow==-1){iNow=0;}

                                };
                            }
                            x=-iNow*aLi[0].offsetWidth;
                            oUl.style.WebkitTransform='translate3d('+x+'px,0,0)';
                            function tend(){
                                bready=true;console.log(bready+'3');
                                oUl.removeEventListener('transitionend',tend,false);
                                if(iNow==aLi.length-1){
                                    iNow=1;
                                    x=-iNow*aLi[0].offsetWidth;oUl.style.WebkitTransition='none';
                                    oUl.style.WebkitTransform='transitionend('+x+'px,0,0)';
                                }
                                if(iNow==0){
                                    iNow=aLi.length-2;
                                    x=-iNow*aLi[0].offsetWidth;oUl.style.WebkitTransition='none';
                                    oUl.style.WebkitTransform='transitionend('+x+'px,0,0)';
                                }
                                haha=iNow;
                            }
                            oUl.addEventListener('transitionend',tend,false);
                        }
                        document.addEventListener('touchmove',fnmove,false);
                        document.addEventListener('touchend',fnend,false);
                        ev.preventDefault();
                    },false);

                });
                $('.mask,.closebtn').click(function(){
                    $('#bigimg').hide();
                    $('.mask').hide();
                    $('.closebtn').hide();
                    $('.fengcai').css('z-index',8);
                }); 
        };
        function duibi(obj){
            if($(obj).width()>$(obj).height()){
                $(obj).css('height','100%').css('margin-left',-($(obj).width()-wimgLI)/2+'px');
            }else{
                $(obj).css('width','100%').css('margin-top',-($(obj).height()-wimgLI)/2+'px');
            }
        }

    var imgnum = $('#smallimg img').length;
        $('#smallimg img').each(function(){
            var _self = $(this);
            var _imgObj = new Image();
            _imgObj.onload = function(){
                resetImageWidthAndHeight(_self, _imgObj);
            };
            _imgObj.src = _self.attr('data-src');
            if(_imgObj.src.indexOf('undefined')!=-1){
                _imgObj.src='http://i0.jrjimg.cn/zqt-red-1000/focus/itg_logo_wx_300_300.png';
            }
        });
    $(window).scroll(function () {
        if ($(document).scrollTop()+$(window).height() >= parseInt($('body').height())-10) {
            $('.app-download').addClass('hide');
        } else {
            $('.app-download').removeClass('hide');
        }
    })
    </script>
